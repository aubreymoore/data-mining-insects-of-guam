<emphasis bold="true" box="[298,816,703,743]" pageId="2" pageNumber="43">
6. 
<taxonomicName _evidence="statusLabel" _step="newLabel1" authorityName="Bernhauer" authorityYear="1942" box="[361,809,703,743]" class="Insecta" family="Staphylinidae" genus="Palaminus" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="43" phylum="Arthropoda" rank="species" species="minutissimus" status="new species">Palaminus minutissimus</taxonomicName>
