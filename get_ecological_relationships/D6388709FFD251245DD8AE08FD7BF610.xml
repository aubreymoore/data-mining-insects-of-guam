<taxonomicName id="99914D9CFFD251245C5FAE08FBC6FEEB" authority="Zimmerman, 1942" authorityName="Zimmerman" authorityYear="1942" box="[772,1084,338,381]" class="Insecta" family="Curculionidae" genus="Neoampagia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="34" pageNumber="107" phylum="Arthropoda" rank="genus" status="new genus">NEOAMPAGIA</taxonomicName>
,
