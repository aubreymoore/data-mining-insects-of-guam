<collectingMunicipality id="6BC1ACD4E95DFFB7FBB2810797C9002B" box="[1149,1471,826,868]" pageId="22" pageNumber="185">National Museum</collectingMunicipality>
</materialsCitation>
<materialsCitation id="3B723CF3E95DFFB7FA1D810791FF00D8" ID-GBIF-Occurrence="3988005301" collectingDate="1911" collectorName="D. T. Fullaway" country="Guam" location="Bishop Museum" pageId="22" pageNumber="185" specimenCount="1">
and one in 
<location id="8EC56075E95DFFB7FEEC8150903B00D8" LSID="urn:lsid:plazi:treatment:03B387B8E95DFFB7FED28300909C04A1:8EC56075E95DFFB7FEEC8150903B00D8" box="[291,589,877,919]" pageId="22" pageNumber="185">Bishop Museum</location>
, 
<location id="8EC56075E95DFFB7FDA48150916700D8" LSID="urn:lsid:plazi:treatment:03B387B8E95DFFB7FED28300909C04A1:8EC56075E95DFFB7FDA48150916700D8" box="[619,785,877,919]" pageId="22" pageNumber="185">Fullaway</location>
