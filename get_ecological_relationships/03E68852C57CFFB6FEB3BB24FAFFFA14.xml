</materialsCitation>
<materialsCitation id="3B273319C57CFFB7FEB2BA59FDDCF143" ID-GBIF-Occurrence="3866012301" box="[315,607,2317,2358]" collectingDate="1936-10-17" collectorName="O. H. Swezey" country="Guam" location="Piti" pageId="0" pageNumber="3">
<date id="FFF11F84C57CFFB7FEB2BA59FE3CF143" box="[315,447,2317,2358]" pageId="0" pageNumber="3" value="2017-10">
<collectingDate id="EFB5E66CC57CFFB7FEB2BA59FE3CF143" box="[315,447,2317,2358]" pageId="0" pageNumber="3" value="2017-10">Oct. 17</collectingDate>
