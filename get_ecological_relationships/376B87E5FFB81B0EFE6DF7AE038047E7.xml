among miscellaneous 
<taxonomicName id="78C24D70FFB91B0EFD78FEC0045847A9" authorityName="Linnaeus" authorityYear="1758" box="[674,862,373,415]" class="Magnoliopsida" higherTaxonomySource="GBIF" kingdom="Plantae" order="Coleoptera" pageId="7" pageNumber="157" phylum="Tracheophyta" rank="order">Coleoptera</taxonomicName>
