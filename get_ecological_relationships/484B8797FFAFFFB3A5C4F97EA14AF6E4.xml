<taxonomicName id="07E24D02FFAFFFB3A693F870A182F802" box="[664,933,1961,2003]" class="Insecta" family="Sarcophagidae" genus="Sarcophaga" kingdom="Animalia" order="Diptera" pageId="1" pageNumber="196" phylum="Arthropoda" rank="species" species="subtuberosa">
