, 
<location id="8EC56075E94AFFA0FB128B5C97E40AC3" LSID="urn:lsid:plazi:treatment:03B387B8E94AFFA3FE5B85CB915D0172:8EC56075E94AFFA0FB128B5C97E40AC3" box="[1245,1426,2401,2444]" country="Seychelles" county="The" municipality="The" name="Tuamotus" pageId="1" pageNumber="164">Tuamotus</location>
