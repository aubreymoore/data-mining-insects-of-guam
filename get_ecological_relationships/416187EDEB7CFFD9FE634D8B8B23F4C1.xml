<taxonomicName id="0EC84D78EB7CFFDAFDA14D8B8BC8FA6B" authorityName="Metcalf" authorityYear="1946" box="[529,957,1756,1798]" class="Insecta" family="Cicadellidae" genus="Empoasca" kingdom="Animalia" order="Hemiptera" pageId="37" pageNumber="144" phylum="Arthropoda" rank="species" species="bipunctulata" status="new species">Empoasca bipunctulata</taxonomicName>
