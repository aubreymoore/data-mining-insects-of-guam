<typeStatus id="54F40D80FFF4FFC071E5581BDAEA0D27" box="[1788,1934,2072,2109]" pageId="7" pageNumber="14">holotype</typeStatus>
<specimenCount id="9D4978ABFFF4FFC07539584FDFF20D6B" box="[544,662,2124,2161]" pageId="7" pageNumber="14" type="female">female</specimenCount>
, 
<specimenCount id="9D4978ABFFF4FFC075B6584FD9780D6B" box="[687,1052,2124,2161]" pageId="7" pageNumber="14" type="female">one paratype female</specimenCount>
