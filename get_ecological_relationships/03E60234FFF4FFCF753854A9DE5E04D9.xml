, 
<date id="FFF195E2FFF4FFC0745A581BDEB30D27" box="[835,983,2072,2109]" pageId="7" pageNumber="14" value="1921-05">
<collectingDate id="EFB56C0AFFF4FFC0745A581BDEB30D27" box="[835,983,2072,2109]" pageId="7" pageNumber="14" value="1921-05">May 21</collectingDate>
</date>
, 
<collectedFrom id="3853F0DFFFF4FFC074EA581BDBBC0D27" box="[1011,1752,2072,2109]" pageId="7" pageNumber="14">
