<taxonomicName id="4C4FC8A1FFF6FFC2754052ABDE9C07D6" authority="Moulton, 1942" authorityName="Moulton" authorityYear="1942" box="[601,1016,680,716]" class="Insecta" family="Phlaeothripidae" genus="Haplothrips" kingdom="Animalia" order="Thysanoptera" pageId="5" pageNumber="12" phylum="Arthropoda" rank="species" species="phyllanthi" status="new species">
