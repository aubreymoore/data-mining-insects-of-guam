<date id="FFA4106EE942FFA8FE0A809690210198" box="[453,599,683,727]" pageId="9" pageNumber="172" value="1925-08">
<collectingDate id="EFE0E986E942FFA8FE0A809690210198" box="[453,599,683,727]" pageId="9" pageNumber="172" value="1925-08">Aug. 25</collectingDate>
