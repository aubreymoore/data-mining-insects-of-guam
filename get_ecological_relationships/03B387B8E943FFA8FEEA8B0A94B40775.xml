<quantity id="4CE29B4BE942FFA8FBDF80DD96FB0045" box="[1040,1165,736,778]" metricMagnitude="-2" metricUnit="m" metricValue="6.0" pageId="9" pageNumber="172" unit="mm" value="60.0">60 mm</quantity>
