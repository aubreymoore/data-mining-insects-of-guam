<specimenCount id="9D1CFD27E94BFFA1FE2186FD917707A4" box="[494,769,1216,1259]" pageId="0" pageNumber="163">single specimen</specimenCount>
in 
<collectingCountry id="F30D763EE94BFFA1FC8C86FD91C607A4" box="[835,944,1216,1259]" name="Guam" pageId="0" pageNumber="163">Guam</collectingCountry>
